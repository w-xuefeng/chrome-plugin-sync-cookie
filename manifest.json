{
	"manifest_version": 2,
	"name": "sync-cookie",
	"version": "1.0.0",
	"description": "解决日常开发中 samesite 引起的跨域 cookie 问题",
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "从调试地址同步Cookie",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"cookies",
		"\u003Call_urls\u003E",
		"contextMenus",
		"unlimitedStorage",
		"notifications",
		"storage",
		"clipboardWrite"
	],
	"background": {
		"scripts": [
			"js/background.js"
		]
	}
}